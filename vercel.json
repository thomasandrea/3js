{
    "builds": [
      {
        "src": "projects/bitmama/vite.config.js",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist/bitmama" }
      },
      {
        "src": "projects/morphing/vite.config.js",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist/morphing" }
      }
    ],
    "routes": [
    { "src": "/bitmama/(.*)", "dest": "/dist/bitmama/$1" },
    { "src": "/morphing/(.*)", "dest": "/dist/morphing/$1" },
    { "src": "/(.*)", "dest": "/dist/$1" }
  ]
  }