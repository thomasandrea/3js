
{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/projects/bitmama/(.*)", "destination": "/bitmama/$1" },
    { "source": "/projects/morphing/(.*)", "destination": "/morphing/$1" },
    { "source": "/", "destination": "/index.html" }
  ],
  "builds": [
    { "src": "projects/bitmama/package.json", "use": "@vercel/node" },
    { "src": "projects/morphing/package.json", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" }
  ]
}
