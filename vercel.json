{
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/projects/bitmama/(.*)",
      "dest": "/projects/bitmama/$1"
    },
    {
      "src": "/projects/morphing/(.*)",
      "dest": "/projects/morphing/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}